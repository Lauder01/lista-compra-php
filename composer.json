{
    "name": "deg540/lista-compra-php",
    "description": "A PHP project for managing a shopping list.",
    "type": "project",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Deg540\\ListaCompraPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deg540\\ListaCompraPHP\\Test\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}